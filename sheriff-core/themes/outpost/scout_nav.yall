# themes/outpost/scout_nav.yall
# How this theme maps nav menus â†’ SCOUT::* tokens
# and which built-in menu type they use.

tokens:
  MAIN_NAV:
    menu: "header"
    kind: "header"        # same menu, same style or change later

  DOCS_TREE:
    menu: "left_docs"
    kind: "sidebar"       # nested docs tree

  FOOTER_LEFT:
    menu: "footer_left"
    kind: "footer_flat"   # inline footer links

  FOOTER_RIGHT:
    menu: "footer_right"
    kind: "footer_flat"
